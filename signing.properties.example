# Signing configuration for release builds
# Copy this file to signing.properties and fill in your actual values
# DO NOT commit signing.properties to version control

# Path to your keystore file
STORE_FILE=path/to/your/keystore.jks

# Keystore password
STORE_PASSWORD=your_keystore_password

# Key alias
KEY_ALIAS=your_key_alias

# Key password
KEY_PASSWORD=your_key_password

